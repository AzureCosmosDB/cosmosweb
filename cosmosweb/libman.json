{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [

    {
      "provider": "cdnjs",
      "library": "jquery@3.6.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery.countdown@2.2.0",
      "destination": "wwwroot/lib/jquery.countdown/"
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-validation@1.19.5",
      "destination": "wwwroot/lib/jquery-validation/"
    },
    {
      "provider": "jsdelivr",
      "library": "@abp/jquery-validation-unobtrusive@6.0.0",
      "destination": "wwwroot/lib/abp/jquery-validation-unobtrusive/"
    }
  ]
}